<div class="container">

  <h1 class="text-center my-5">My Tasks</h1>

  <div class="board">
    <% @tasks.each do |task| %>
      <div class="container justify-content-center text-center">
        <h1><%= button_to task.title, tasks_path, class: "task-card" %></h1>
      </div>
    <% end %>
  </div>
</div>
